algoritmo "Delta"
var
   A, C, Delta, B: Real
inicio
      Escreva("Digite o valor de 'A': ")
       Leia(A)
       
      Escreva("Digite o valor de 'B': ")
       Leia(B)
       
      Escreva("Digite o valor de 'C': ")
       Leia(C)
       
      Delta <- (B^2)-4*A*C
      
      Escreva("Digite o valor de Delta é:", Delta)
fimalgoritmo