algoritmo "SalarioFuncionario"
var
     nome: caractere
     Renda: Real
inicio
     Escreva("Nome do funcionario:")
     Leia(nome)
     Escreva("Sálario:")
     Leia(Renda)
     Escreval("O funcionário ", nome ," teve um salário de", Renda,"$ em Maio")
     Escreva("naquela porcaria de farmacia.")
fimalgoritmo